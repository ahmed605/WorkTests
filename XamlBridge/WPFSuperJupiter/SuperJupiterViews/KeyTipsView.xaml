        </StackPanel>

        <TextBlock Style="{StaticResource BaseTextBlockStyle}" FontSize="20" Margin="40" TextAlignment="Center">KeyTips with CommandBar</TextBlock>
      <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}" Margin="40">
